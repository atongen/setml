{
    "name": "setml",
    "bsc-flags": [
        "-bs-no-version-header",
        "-bs-super-errors"
    ],
    "reason": {
        "react-jsx": 2
    },
    "bs-dependencies": [
        "reason-react",
        "bs-fetch",
        "@glennsl/bs-json",
        "bs-websockets",
        "bs-webapi",
        "@glennsl/bs-json"
    ],
    "bs-dev-dependencies": ["@glennsl/bs-jest"],
    "sources": [
        {
            "dir": "src",
            "subdirs": [
                "client",
                "shared"
            ]
        },
        {
            "dir": "__tests__",
            "type": "dev"
        }
    ],
    "package-specs": {
        "module": "es6",
        "in-source": false
    },
    "refmt": 3
}
