{
    "name": "setml",
    "bsc-flags": [
        "-bs-no-version-header",
        "-bs-super-errors"
    ],
    "reason": {
        "react-jsx": 2
    },
    "bs-dependencies": [
        "reason-react",
        "bs-websockets",
        "bs-webapi",
        "@glennsl/bs-json",
        "@jsiebern/bs-material-ui",
        "@glennsl/bs-jest",
        "bs-fetch"
    ],
    "bs-dev-dependencies": [],
    "sources": [
        {
            "dir": "src",
            "subdirs": [
                "client",
                "shared"
            ]
        },
        {
            "dir": "__tests__",
            "type": "dev"
        }
    ],
    "package-specs": {
        "module": "commonjs",
        "in-source": false
    },
    "refmt": 3,
    "ppx-flags": [
        "./node_modules/@jsiebern/bs-material-ui/ppx_withStyles"
    ]
}
